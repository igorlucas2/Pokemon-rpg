    "name": "PokemonTower_1F",
    "dialogs": [
        {
            "title": "PokemonTower_1F_Text_CameToPrayForDepartedClefairy",
            "id": "PokemonTower_1F_EventScript_BaldingMan",
            "text": "I came to pray for my dearly\ndeparted CLEFAIRY.\n\nSniff!\nI'm awash in tears…"
        },
        {
            "title": "PokemonTower_1F_Text_SenseSpiritsUpToMischief",
            "id": "PokemonTower_1F_EventScript_Channeler",
            "text": "I am a CHANNELER.\n\nThere are spirits up to mischief.\nI sense them high in the TOWER."
        },
        {
            "title": "PokemonTower_1F_Text_GrowlitheWhyDidYouDie",
            "id": "PokemonTower_1F_EventScript_Woman1",
            "text": "My GROWLITHE…\nWhy did you die?"
        },
        {
            "title": "PokemonTower_1F_Text_ComeToPayRespectsGirl",
            "id": "PokemonTower_1F_EventScript_Woman2",
            "text": "Did you come to pay respects?\n\nBless your POKéMON-loving heart,\ngirl."
        },
        {
            "title": "PokemonTower_1F_Text_ErectedInMemoryOfDeadMons",
            "id": "PokemonTower_1F_EventScript_WorkerF",
            "text": "POKéMON TOWER was erected in the\nmemory of POKéMON that died."
        }
    ],
