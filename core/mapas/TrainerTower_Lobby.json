    "name": "TrainerTower_Lobby",
    "dialogs": [
        {
            "title": "TrainerTower_Lobby_Text_StairsTougherThanAnyBattle",
            "id": "TrainerTower_Lobby_EventScript_BaldingMan",
            "text": "Gasp, gasp…\nGasp…\n\nNever mind battling! These stairs…\nThey're tougher than any battle…"
        },
        {
            "title": "TrainerTower_Lobby_Text_WonderWhatKindsOfTrainers",
            "id": "TrainerTower_Lobby_EventScript_CooltrainerF",
            "text": "I'm here to see how good I am.\n\nI wonder what kinds of TRAINERS\nare waiting for me?\n\nIt's nerve-racking!"
        },
        {
            "title": "TrainerTower_Lobby_Text_WelcomeToTrainerTower",
            "id": "TrainerTower_Lobby_EventScript_EntryTrigger",
            "text": "Hello!\n\nWelcome to TRAINER TOWER where\nTRAINERS gather from all over!\n\nTRAINERS from all over the world\ngather here to battle.\n\nLet me see…\n\nRight now, there are TRAINERS only\nup to Floor {STR_VAR_1}."
        },
        {
            "title": "Text_MayIHelpYou",
            "id": "TrainerTower_Lobby_EventScript_MartClerk",
            "text": "Hi, there!\nMay I help you?"
        },
        {
