        {
            "title": "ViridianCity_Text_GoShoppingInPewterOccasionally",
            "id": "ViridianCity_EventScript_Woman",
            "text": "I go shopping in PEWTER CITY\noccasionally.\n\nI have to take the winding trail in\nVIRIDIAN FOREST when I go."
        },
