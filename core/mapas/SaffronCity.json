    "name": "SaffronCity",
    "dialogs": [
        {
            "title": "SaffronCity_EventScript_BattleGirl",
            "id": "SaffronCity_EventScript_BattleGirl",
            "text": "TODO: SaffronCity_EventScript_BattleGirl"
        },
        {
            "title": "SaffronCity_Text_FlewHereOnPidgeot",
            "id": "SaffronCity_EventScript_Boy",
            "text": "I flew here on my PIDGEOT when\nI read about SILPH.\n\nIt's already over?\nI missed the media action…"
        },
        {
            "title": "SaffronCity_Text_CitySign",
            "id": "SaffronCity_EventScript_CitySign",
            "text": "SAFFRON CITY\nShining, Golden Land of Commerce"
        },
        {
            "title": "SaffronCity_Text_FightingDojo",
            "id": "SaffronCity_EventScript_DojoSign",
            "text": "FIGHTING DOJO"
        },
        {
            "title": "SaffronCity_Text_ImASecurityGuard",
            "id": "SaffronCity_EventScript_DoorGuardGrunt",
            "text": "I'm a security guard.\n\nSuspicious kids I don't allow in!"
        },
        {
            "title": "SaffronCity_Text_GymSign",
            "id": "SaffronCity_EventScript_GymSign",
            "text": "SAFFRON CITY POKéMON GYM\nLEADER: SABRINA\nThe Master of PSYCHIC POKéMON!"
        },
        {
            "title": "SaffronCity_Text_PeopleComingBackToSaffron",
            "id": "SaffronCity_EventScript_Lass",
            "text": "People fled from here in droves\nwhen those ROCKETS came.\n\nThey should be flocking back to\nSAFFRON now."
        },
        {
            "title": "SaffronCity_Text_SawRocketBossEscaping",
            "id": "SaffronCity_EventScript_Man",
            "text": "I saw the ROCKET BOSS escaping\nSILPH's building."
        },
        {
            "title": "SaffronCity_Text_MrPsychicsHouse",
            "id": "SaffronCity_EventScript_MrPsychicsHouseSign",
            "text": "MR. PSYCHIC'S HOUSE"
        },
        {
            "title": "SaffronCity_Text_Pidgeot",
            "id": "SaffronCity_EventScript_Pidgeot",
            "text": "PIDGEOT: Bi bibii!"
        },
        {
            "title": "SaffronCity_Text_WhatDoYouWantGetLost",
            "id": "SaffronCity_EventScript_RocketGrunt1",
            "text": "What do you want?\nGet lost!"
        },
        {
            "title": "SaffronCity_Text_BossTakeTownForTeamRocket",
            "id": "SaffronCity_EventScript_RocketGrunt2",
            "text": "BOSS said he'll take this town\nin the name of TEAM ROCKET!"
        },
        {
            "title": "SaffronCity_Text_DontGetDefiantOrIllHurtYou",
            "id": "SaffronCity_EventScript_RocketGrunt3",
            "text": "Don't get defiant!\nOr I'll have to hurt you!"
        },
        {
            "title": "SaffronCity_Text_SaffronBelongsToTeamRocket",
            "id": "SaffronCity_EventScript_RocketGrunt4",
            "text": "SAFFRON belongs to TEAM ROCKET!"
        },
        {
            "title": "SaffronCity_Text_CriminalLifeMakesMeFeelAlive",
            "id": "SaffronCity_EventScript_RocketGrunt5",
            "text": "My life as a criminal makes me feel\nso alive!"
        },
        {
            "title": "SaffronCity_Text_WatchWhereYoureWalking",
            "id": "SaffronCity_EventScript_RocketGrunt6",
            "text": "Ow!\nWatch where you're walking!"
        },
        {
            "title": "SaffronCity_Text_WeCanExploitMonsAroundWorld",
            "id": "SaffronCity_EventScript_RocketGrunt7",
            "text": "With SILPH under control, we can\nexploit POKéMON around the world!\n\nWe'll get stinking rich, yeahah!"
        },
        {
            "title": "SaffronCity_Text_SilphCoSign",
            "id": "SaffronCity_EventScript_SilphCoSign",
            "text": "SILPH CO. OFFICE BUILDING"
        },
        {
            "title": "SaffronCity_Text_SilphsLatestProduct",
            "id": "SaffronCity_EventScript_SilphProductSign",
            "text": "SILPH's latest product!\nRelease to be determined…"
        },
        {
            "title": "SaffronCity_Text_TrainerFanClubSign",
            "id": "SaffronCity_EventScript_TrainerFanClubSign",
            "text": "POKéMON TRAINER FAN CLUB\n\nMany TRAINERS have scribbled their\nnames on this sign."
        },
        {
            "title": "SaffronCity_Text_FullHealCuresStatus",
            "id": "SaffronCity_EventScript_TrainerTips1",
            "text": "TRAINER TIPS\n\nFULL HEAL cures all ailments like\nburns, paralysis, poisoning,\nfreezing, and sleep.\n\nIt costs a bit more, but it's more\nconvenient than buying other items."
        },
        {
            "title": "SaffronCity_Text_GreatBallImprovedCatchRate",
            "id": "SaffronCity_EventScript_TrainerTips2",
            "text": "TRAINER TIPS\n\nThe new GREAT BALL offers improved\ncatching rates.\n\nTry it on those hard-to-catch\nPOKéMON."
        },
        {
            "title": "SaffronCity_Text_YouBeatTeamRocket",
            "id": "SaffronCity_EventScript_WorkerM",
            "text": "You beat TEAM ROCKET all alone?\nThat's amazing!"
        },
        {
            "title": "SaffronCity_Text_SafeToGoOutAgain",
            "id": "SaffronCity_EventScript_Youngster",
            "text": "Yeah! TEAM ROCKET is gone!\nIt's safe to go out again!"
        }
    ],
